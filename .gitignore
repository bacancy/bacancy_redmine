# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Ignore JetBrains RubyMine IDE settings
/.idea

# Ignore PaperClip images
/public/system

# Ignore compiled assets
public/

# Ignore OSX folder attributes
.DS_Store

# Ignore RVM profiles
.rvmrc

# Ignore simplecov results
coverage

# Ignore application configuration
/config/application.yml

#gem.lock file
Gemfile.lock

# Ignore Heroku Environment Vars
# See https://devcenter.heroku.com/articles/config-vars#local-setup
.env